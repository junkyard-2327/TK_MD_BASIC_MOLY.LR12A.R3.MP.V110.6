******* SBP Document END **************************************************************************/
    SBP_FEATURE_DEF(SBP_STATUS_CDG_AVOIDANCE, SBP_ATTR_NONE, "CDG EVDO Channel Avoidance Feature", "Not enabled", "Enabled", 0)

