    SBP_FEATURE_DEF(SBP_START_TD_TIMER_WITH_T3402_TIMER_VALUE, SBP_ATTR_NONE, "Start TD timer with T3402 timer value", "Not to start TD timer with T3402 timer value", "To start TD timer with T3402 timer value", 1)
