    SBP_FEATURE_DEF(SBP_LTE_CONN_ALWAYS_RCV_UE_PCH, SBP_ATTR_NONE, "Allow NW to notify incoming call with PAGING in LTE CONNECTED mode", "Not Enabled", "Enabled", 0)
