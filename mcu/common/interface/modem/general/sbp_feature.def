#if defined(__DISABLE_STORED_LIST_FOR_4G2G_CSFB__)	
    SBP_FEATURE_DEF(SBP_GAS_REDIRECT_USE_STORED_LIST, SBP_ATTR_NONE, "Use stored list while MO-MT CSFB from LTE to GSM", "Not enabled", "Enabled", 0)
