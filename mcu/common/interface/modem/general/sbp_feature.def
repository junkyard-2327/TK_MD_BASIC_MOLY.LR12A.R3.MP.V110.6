<SBP title="SBP_PREFER_HPLMN_IN_SHARED_NETWORK">
    <DESCRIPTION>
        <PARAG> UE originally camp on CDMA 2000. And cell re-selection start when LTE share cell are detected
				The RPLMN/EPLMN is deleted because previous rejection from network, reject cause #9.
				Without RPLMN/EPLMN/Selected PLMN, UE has no preference on the share cell and choose the 1st one.
				This cause cell re-selection fail. SPRINT expect that UE select 312-530 (EHPLMN) over 310-120 in share network
                When SBP_PREFER_HPLMN_IN_SHARED_NETWORK is ON and there is no RPLMN/EPLMN/Selected PLMN in share network or in UE side, 
                UE prefer to select the HPLMN instead of the 1st one of the share network.
        </PARAG>
    </DESCRIPTION>
    <CATEGORY>NWSEL</CATEGORY>
    <CONFIG>
        <VALUE value="0">Disabled</VALUE>
        <VALUE value="1">Enabled</VALUE>
    </CONFIG>
    <DEFAULT>0</DEFAULT>
    <OP>SPRINT</OP>
    <DEPENDENCY>This SBP can be controlled by compile option __PREFER_HPLMN_IN_SHARED_NETWORK__</DEPENDENCY>
    <NOTE>None</NOTE>
    <AUTHOR>mtk13581</AUTHOR>
</SBP>
******* SBP Document END **************************************************************************/
#ifdef __PREFER_HPLMN_IN_SHARED_NETWORK__
   SBP_FEATURE_DEF(SBP_PREFER_HPLMN_IN_SHARED_NETWORK, SBP_ATTR_NONE, "Prefer EHPLMN in share NW if there is no RPLMN/EPLMN/Selected PLMN", "Disabled", "Enabled", 1)
#else
   SBP_FEATURE_DEF(SBP_PREFER_HPLMN_IN_SHARED_NETWORK, SBP_ATTR_NONE, "Prefer EHPLMN in share NW if there is no RPLMN/EPLMN/Selected PLMN", "Disabled", "Enabled", 0)
#endif

